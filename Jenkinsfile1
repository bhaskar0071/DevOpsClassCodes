pipeline {
    agent any
	tools{
		maven "Maven3"
	}
        
    stages {
        stage('Static Analysis') {
            steps {
                // Checkout the repository from GitHub
                git 'https://github.com/bhaskar0071/DevOpsClassCodes.git'
                }
            }
        }

        stage('Code Compilation and Testing') {
            parallel {
                stage('Code Compilation') {
                    steps {
                        echo 'compiling'
                        sh 'mvn compile'
                    }
                }

                stage('Code Testing') {
                    steps {
                        git 'https://github.com/bhaskar0071/DevOpsClassCodes.git'
                        sh 'mvn test'
                    }
                }
            }
        }
    }
}
              
